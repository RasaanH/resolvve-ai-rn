{
  "expo": {
    "name": "Balance GPT",
    "slug": "resolvve-ai-rn",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/balancegpt_adaptive_icon.png",
    "scheme": "myapp",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/balancegpt_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#262222"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/balancegpt_adaptive_icon.png",
        "backgroundColor": "#262222"
      },
      "permissions": ["com.android.vending.BILLING"],
      "package": "com.rasaanorg.balancegpt"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#262222",
          "image": "./assets/images/balancegpt_splash.png",
          "dark": {
            "image": "./assets/images/balancegpt_splash.png",
            "backgroundColor": "#262222"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8f09bc88-f49f-4d10-be42-a1ed69b9f99f"
      }
    },
    "owner": "rasaan-org"
  }
}
